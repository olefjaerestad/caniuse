{
  "author": "Ole Kristian Fjaerestad",
  "bugs": {
    "url": "https://github.com/olefjaerestad/caniuse/issues"
  },
  "dependencies": {
    "express": "^4.17.1",
    "googleapis": "^66.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "description": "Check if you can use a certain CSS or JS feature on your site based on browser usage data from Google Analytics.",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@snowpack/plugin-typescript": "^1.1.1",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "rollup": "^2.35.1",
    "snowpack": "^2.18.5",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "homepage": "https://github.com/olefjaerestad/caniuse#readme",
  "keywords": [
    "browsersupport",
    "googleanalytics"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "name": "caniuse",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olefjaerestad/caniuse.git"
  },
  "scripts": {
    "build": "rm -rf build && npm run build:rollup",
    "build:rollup": "IS_MOCK_MODE=false NODE_ENV=production rollup --config",
    "dev": "rm -rf dist && concurrently \"npm run dev:watch\" \"npm run dev:serve\"",
    "dev:mock": "rm -rf dist && concurrently \"IS_MOCK_MODE=true npm run dev:watch\" \"IS_MOCK_MODE=true npm run dev:serve\"",
    "dev:serve": "NODE_ENV=development nodemon --experimental-specifier-resolution=node scripts/dev.js",
    "dev:watch": "snowpack build --polyfill-node --watch",
    "mock:generate": "node scripts/generate-mock-data.js",
    "start": "NODE_ENV=production node --experimental-specifier-resolution=node scripts/prod.js"
  },
  "type": "module",
  "version": "0.0.1"
}
